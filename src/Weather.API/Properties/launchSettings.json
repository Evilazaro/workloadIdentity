{
  "profiles": {
    "WSL": {
      "commandName": "WSL2",
      "distributionName": ""
    },
    "Http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost:6060/swagger/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "remoteDebugEnabled": true,
      "nativeDebugging": true,
      "applicationUrl": "http://localhost:6060/"
    },
    "Https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "https://localhost:6061/swagger/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "remoteDebugEnabled": true,
      "nativeDebugging": true,
      "applicationUrl": "https://localhost:6061/",
      "useSSL": true
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger/",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}